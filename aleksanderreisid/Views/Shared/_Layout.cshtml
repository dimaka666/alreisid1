<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Free Responsive Template #10 - large palm</title>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.flexslider-min.js"></script>
    <script src="~/Scripts/common.js"></script>
    <script src="~/Scripts/modernizr.custom.53451.js"></script>
    @Styles.Render("~/Content/styles.css")
    @Styles.Render("~/Content/Site.css")
  
    @Styles.Render("~/Content/style1.css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    </head>
    <body>
        <div class="wrapper">
            <header>
                <div class="headerContainer">
                    <h1>@Html.ActionLink("Aleksandr Reisid", "Index", "Home")    </h1>
                    <div id="navcontainer">
                        <div id="navlist">
                            <div class="naviList">
                                <span style="position: relative;padding-top: 50px;">
                                    @Html.MenuLink("Главная", "Index", "Home")
                                </span>
                            </div>
                            <div class="naviList">
                                <span style="position: relative;padding-top: 50px;">
                                    @Html.MenuLink("Визы", "Index", "Visa")
                                </span>
                            </div>
                            <div class="naviList">
                                <span style="position: relative;padding-top: 50px;">
                                    @Html.MenuLink("Туры", "Index", "Trip")
                                </span>
                            </div>
                            <div class="naviList">
                                <span style="position: relative;padding-top: 50px;">
                                    @Html.MenuLink("О Нас", "About", "Home")
                                </span>
                            </div>
</div>
</div>
<br class="clearfloat" />
<!-- begin .slider -->
<div class="slider">
    <div class="slogan">
        <img src="~/images/slogan0.png" alt="sloganImg" />
        <img src="~/images/slogan1.png" alt="sloganImg" />
       <img src="~/images/slogan.png" alt="sloganImg"/>
    </div>
</div>
<br class="clearfloat" />
<!-- end .slider -->
</div>
</header>
<div class="container body-content">
    @RenderBody()
</div>
</div>

@RenderSection("scripts", required: false)

</body>
</html>
